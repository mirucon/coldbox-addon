{
  "name": "coldbox-addon",
  "description": "The powered addon for the Coldbox theme.",
  "version": "1.2.5",
  "author": "Toshihiro Kanai <i@miruc.co>",
  "devDependencies": {
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.1",
    "postcss": "8.4.31",
    "postcss-cli": "10.1.0",
    "postcss-for": "2.1.1",
    "postcss-nested": "7.0.2",
    "postcss-simple-vars": "7.0.1",
    "prettier": "3.0.1"
  },
  "resolutions": {
    "natives": "^1.1.6"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "postcss inc/amp-style.scss -o inc/amp-style.css",
    "watch": "postcss inc/amp-style.scss -o inc/amp-style.css -w",
    "dist": "mkdir -p dist/ && mkdir -p dist/inc && cp inc/*.php dist/inc/ && cp coldbox-addon.php dist/coldbox-addon.php && cp readme.txt dist/readme.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirucon/coldbox-addon"
  },
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/mirucon/coldbox-addon/issues"
  },
  "homepage": "https://coldbox.miruc.co/",
  "browserslist": [
    "> 1%",
    "not ie 11"
  ]
}
